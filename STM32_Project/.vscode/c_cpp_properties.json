{
    "configurations": [
        {
            "name": "STM32",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/CMSIS/**",
                "${workspaceFolder}/User/**",
                "${workspaceFolder}/Drivers/UserLib/inc/**",
                "${workspaceFolder}/STM32F10x_StdPeriph_Driver/inc/**"
            ],
            "defines": [
                "USE_STDPERIPH_DRIVER",
                "STM32F10X_HD"
            ],
            "compilerPath": "/usr/bin/arm-none-eabi-gcc",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "linux-gcc-arm"
        }
    ],
    "version": 4
}
