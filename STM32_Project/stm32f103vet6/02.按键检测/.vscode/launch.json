{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug STM32F103C8",
      "type": "cortex-debug",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "executable": "${workspaceFolder}/build/stm32f103c8.elf",
      "servertype": "openocd",
      "configFiles": ["openocd/stm32.cfg"],
      "runToEntryPoint": "main",
      "device": "STM32F103C8",
      "svdFile": "${workspaceFolder}/STM32F103.svd",
      "preLaunchTask": "flashall",
      "gdbPath": "/usr/bin/arm-none-eabi-gdb"
    }
  ]
}
